{
  "name": "super-es6-boilerPlate",
  "version": "1.0.0",
  "description": "Gulp-Browserify-Babelify-Jest ES6 BoilerPlate w/ gulp-sass and gulp-eslint",
  "main": "app.js",
  "scripts": {
    "test": "gulp lint; BABEL_JEST_STAGE=0 jest;"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testDirectoryName": "specs",
    "testPathDirs": [
      "<rootDir>/specs"
    ],
    "unmockedModulePathPatterns": [],
    "globals": {
      "__APPDIR__": "<rootDir>/src/javascripts",
      "__SPECSDIR__": "<rootDir>/specs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:RohitRox/ES6-Boilerplate.git"
  },
  "author": "RohitRox",
  "license": "ISC",
  "dependencies": {
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "browserify-shim": "^3.8.10"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "babel-jest": "^5.3.0",
    "babel-preset-es2015": "^6.1.4",
    "debug": "^2.2.0",
    "eslint": "^1.7.3",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-plugin-react": "^3.6.3",
    "gulp": "^3.9.0",
    "gulp-browserify": "^0.5.1",
    "gulp-concat": "^2.6.0",
    "gulp-eslint": "^1.0.0",
    "gulp-sass": "^2.1.0",
    "gulp-serve": "^1.2.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "jest-cli": "^0.6.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.5.0"
  }
}
